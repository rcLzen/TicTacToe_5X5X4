        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="cmbLogin" HorizontalAlignment="Left" Margin="203,128,0,0" VerticalAlignment="Top" Width="120" IsEditable="True"/>
        <Button x:Name="btnClear" Content="Clear" HorizontalAlignment="Left" Margin="166,214,0,0" VerticalAlignment="Top" Width="75" Click="btnClear_Click"/>
        <Button x:Name="btnSubmit" Content="Submit" HorizontalAlignment="Left" Margin="354,214,0,0" VerticalAlignment="Top" Width="75" Click="btnSubmit_Click"/>
        <Label x:Name="lblName" Content="" HorizontalAlignment="Left" Margin="139,37,0,0" VerticalAlignment="Top" Width="241"/>

    </Grid>
</Window>
